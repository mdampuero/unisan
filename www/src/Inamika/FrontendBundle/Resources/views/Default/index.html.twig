{% block javascripts %}
{{ parent()}}
    <script>
        var filters={{ filters | json_encode | raw }};
        console.log(filters);
        function selectCategory(category){
            $(".icon-svg").removeClass('active');
            $("#btn"+category).addClass('active');
            switch (category) {
                case 'forIndustry':
                    $(".forIndustry").fadeIn();
                    $(".forHome").hide();
                    $(".forEvent").hide();
                    break;
                case 'forHome':
                    $(".forIndustry").hide();
                    $(".forHome").fadeIn();
                    $(".forEvent").hide();
                    break;
                case 'forEvent':
                    $(".forIndustry").hide();
                    $(".forHome").hide();
                    $(".forEvent").fadeIn();
                    break;
                default:
                    break;
            }
        }

        function selectFilter(category,index){
            $("."+category+" ul li").removeClass("active");
            if(index > -1){
                $("."+category+" ul li[data-name='"+filters[category][index].name+"']").addClass("active");
                $("."+category+".card").hide();
                for(var i=0; i<filters[category][index].services.length; i++){
                    $("."+category+".card[data-id='"+filters[category][index].services[i]+"']").fadeIn();
                }
            }else{
                $("."+category+" ul li[data-name='all']").addClass("active");
                $("."+category+".card").fadeIn();
            }
        }
        $(function (){
            selectCategory('forIndustry');
        })
    </script>
{% endblock %}